<liferay-portlet-app>
	<portlet>
		<portlet-name>artists</portlet-name>
		<icon>/icons/artists.png</icon>
		<instanceable>false</instanceable>
		<scopeable>true</scopeable>
		<header-portlet-css>/css/artists.css</header-portlet-css>
		<footer-portlet-javascript>/js/artists.js</footer-portlet-javascript>
